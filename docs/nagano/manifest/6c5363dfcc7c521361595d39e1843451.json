    "metadata": [
        {
            "label": "タイトル（ヨミ）",
            "value": "アイタイシサンブマシチンセンシラベチョウ"
        },
        {
            "label": "分野",
            "value": "歴史"
        },
        {
            "label": "制作年（和暦）",
            "value": "明治3年"
        },
        {
            "label": "施設名",
            "value": "県立長野図書館"
        },
        {
