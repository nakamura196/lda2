            "label": "分野",
            "value": "歴史"
        },
        {
            "label": "制作年（和暦）",
            "value": "元文4年"
        },
        {
            "label": "施設名",
            "value": "県立長野図書館"
        },
        {
            "label": "資料コード",
            "value": "02OD5050080000"
