    "metadata": [
        {
            "label": "資料番号",
            "value": "1329"
        },
        {
            "label": "名称よみ",
            "value": "ぶっせつまかしゅぶつみょうらくきょう"
        },
        {
            "label": "作者",
            "value": "亀田鵬斎著　谷文晁画　酒井抱一画"
        },
        {
            "label": "年代",
            "value": "文政6年"
        },
        {
            "label": "解説",
            "value": "仏典のパロディの形式で、酒が人生に慰めと喜びをもたらすことを述べています。巻頭に谷文晁筆の劉伯倫夫妻が酒瓶を礼拝する画、巻末に酒井抱一筆の李白が酒瓶によりかかる画を添え、造本も経典風の折本仕立てにされています。"
        },
        {
            "label": "作者よみ",
            "value": "かめだぼうさい　たにぶんちょう　さかいほういつ"
        },
        {
            "label": "員数",
            "value": "1冊"
        },
        {
            "label": "西暦",
            "value": "1823"
        },
        {
            "label": "中分類ID",
            "value": "0"
        },
        {
            "label": "形態/材質/品質",
            "value": "版折本"
        }
    ],
